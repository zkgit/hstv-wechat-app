.touchbox{width: 100%;height:533rpx;background: #363636;}
.touchboxactive{box-shadow: 6px 10px 24px rgba(0,0,0,0.3) inset,-3px -5px 18px rgba(255,255,255,.1) inset;}
.touchbox .touch_direction{width:170rpx;height:138.19rpx;top:0;left:0;right:0;bottom:0;margin: auto;display: none;}
.touchbox .touch_direction.up{
    transform: rotate(-90deg);
}
.touchbox .touch_direction.bottom{
   transform: rotate(90deg);
}
.touchbox .touch_direction.left{
   transform: rotate(180deg);
}
.touchbox .touch_direction.right{
   transform: rotate(0);
}
.touchbox .touch_ok{width:152rpx;height:152rpx;display: none;}