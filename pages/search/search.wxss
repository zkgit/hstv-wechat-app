.searchRank{border:none;position: fixed;top: 0;left:0;width: 100%;z-index: 3;}
.searchRank .form{width: 100%;height:68rpx;}
.searchRank .form .cl{width:80%;margin:0;border-radius:34rpx;padding:0 50rpx 0 64rpx;line-height: 68rpx;height:68rpx;}
.searchRank .form .cl icon{top:19rpx;left:20rpx;}
.searchRank .form .cl .jian{width:14rpx;height:8rpx;margin:0 16rpx 6rpx}
.searchRank .form .cl .qx_btn{right:20rpx;left:auto;}
.searchRank .form .btn{width:20%;line-height: 68rpx;}
.searchRank input{line-height: 68rpx;height:68rpx;}
.searchbox{margin-top: 118rpx;border-top:2rpx solid #e5e5e5;}
.searchbox .his{padding: 40rpx 20rpx 20rpx;}
.searchbox .his .hisli{display: inline-block;margin-right:28rpx;margin-bottom:20rpx;white-space: nowrap;padding: 0 20rpx;line-height:54rpx;height:54rpx;border-radius:6rpx;}

.searchbox .hot_ul{line-height: 60rpx;flex-wrap: wrap;}
.searchbox .hot_ul image{height:30rpx;width:30rpx;}
.tab-bar{height:80rpx;line-height:34rpx;}
.tab-bar .tab-active::after{width:46rpx;height:8rpx;font-size: 38rpx;background:#f45335;border-radius: 2rpx;content:'';display:block;margin: 18rpx auto 0;}
