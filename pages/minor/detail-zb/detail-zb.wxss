.stv-container {
 
}


.stv-container .tab-bar {
  position: relative;
  font-size: 32rpx;
  height: 78rpx;
  /* line-height: 18rpx; */
  white-space: nowrap;
}

.stv-container .tab-bar .tab {
  align-items: center;
  justify-content: center;
  height: 78rpx;
  display: inline-block;
  border-bottom: 4rpx solid transparent;
  margin: 0 30rpx;
  box-sizing: content-box;
}

.stv-container .tab-bar .tab-active {
   color: #f45335;
  /* border-bottom: 4rpx solid #f45335;    */
}
.stv-container .tab-bar .tab-active .i{
    position: absolute;
    left: 50%;
    margin-left: -24rpx;
    width: 46rpx;
    height: 4rpx;
    display: block;
    background: #f45335;
    border-radius: 6px;     
    bottom: 0;
}

.stv-container .dateBox {
  border-top: 1px solid #eeeeee;
  border-bottom: 1px solid #eeeeee;
  align-items: center;
}

.stv-container .dateBox .dateBar {
  white-space: nowrap;
}
.stv-container .dateBox .dateTab {
  width: 98rpx;
  display: inline-block;  
}

.stv-container .tdCx {
  border-radius: 6rpx;  
}

.stv-container .scroll-view {
  width: 710rpx;
  height: 460rpx;
  margin: 0 40rpx;
}

.stv-container .itemText{
  border-box: border-box;
  margin: 4rpx  10rpx 0;
  padding-left:12rpx;
  height: 60rpx;
  width: 160rpx;
  line-height: 60rpx;
  border-radius: 6rpx;
  box-sizing: border-box;
}


